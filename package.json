{
  "name": "collector",
  "author": "erriy <erriy@msn.com>",
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "https://github.com/Erriy/collector#readme",
  "bugs": {
    "url": "https://github.com/Erriy/collector/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Erriy/collector.git"
  },
  "main": "lib/index.js",
  "bin": "bin/cli.js",
  "files": [
    "lib",
    "bin"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:staged": "lint-staged",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "test": "mocha"
  },
  "devDependencies": {
    "@commitlint/config-angular": "^12.1.4",
    "@types/mocha": "^8.2.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.2.4",
    "commitlint": "^12.1.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.28.0",
    "eslint-plugin-mocha": "^9.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^9.0.0",
    "prettier": "^2.3.1",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
    "commit-msg": "commitlint -E GIT_PARAMS",
    "pre-commit": "npm run lint:staged",
    "post-commit": "node script/post-commit.js"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{css,json,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^0.21.3",
    "cheerio": "^1.0.0-rc.10",
    "commander": "^8.1.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^8.1.2",
    "inquirer-autocomplete-prompt": "^1.4.0",
    "youtube-dl-exec": "^1.2.5"
  }
}
